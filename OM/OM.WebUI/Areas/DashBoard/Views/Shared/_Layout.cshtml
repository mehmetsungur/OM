@using OM.Entity.Domain
@{
    User activeUser = Session["ActiveUser"] as User;
}
<html>
<head>
    <meta charset="utf-8" />
    <title>OLGUN MAKİNA - Dashboard Paneli</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="shortcut icon" href="~/Areas/DashBoard/Content/img/Default/favicon.ico">
    @Styles.Render("~/css/Datatables")
    @Styles.Render("~/css/Default")
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
</head>
<body>
    <div id="wrapper">
        <div class="navbar-custom">
            @Html.Partial("_PartialNavbarCustom", activeUser)
        </div>
        <div class="left-side-menu">
            @Html.Partial("_PartialLeftSideMenu")
        </div>
        <div class="content-page">
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            @Html.Partial("_PartialDashBoardFooter")
        </div>
    </div>
    @Html.Partial("_PartialRightBar", activeUser)
    @Scripts.Render("~/js/Default")
    @Scripts.Render("~/js/Custom/Create")
    @Scripts.Render("~/js/Agenda")
</body>
</html>